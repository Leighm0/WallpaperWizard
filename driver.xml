	<control>lua_gen</control>
	<driver>DriverWorks</driver>
	<proxy>WallpaperWizard</proxy>
	<combo>true</combo>
	<events/>
	<config>
		<documentation>
	Wallpaper Wizard by Welton Cartel

	This driver allows setting UI Wallpaper of selected rooms to custom wallpaper via programming.


	RELEASE NOTES:

	Version 1: Initial Release
	Version 2: Code clean up
		</documentation>
		<properties>
			<property>
				<name>Driver Information</name>
				<type>LABEL</type>
				<default>Driver Information</default>
			</property>
			<property>
				<name>Debug Mode</name>
				<type>LIST</type>
				<items>
					<item>Off</item>
					<item>Print</item>
					<item>Log</item>
					<item>Print and Log</item>
				</items>
				<default>Off</default>
				<readonly>false</readonly>
			</property>
		</properties>
		<script jit="1" file="driver.lua"/>
		<actions/>
		<commands>
			<command>
				<name>Set Wallpaper</name>
				<description>Set the Wallpaper on PARAM1 to: PARAM2</description>
				<params>
					<param>
						<name>Room Selection</name>
						<type>DEVICE_SELECTOR</type>
						<items>
							<item>roomdevice.c4i</item>
						</items>
						<multiselect>true</multiselect>
						<default/>
					</param>
					<param>
						<name>Wallpaper</name>
						<type>CUSTOM_SELECT:GetWallpaperList</type>
						<default/>
					</param>
				</params>
			</command>
		</commands>
	</config>
